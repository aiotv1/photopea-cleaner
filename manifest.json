{
  "manifest_version": 3,
  "name": "Photopea Cleaner",
  "version": "1.0",
  "description": "Automatically removes the annoying consent element from Photopea.",
  "content_scripts": [
    {
      "matches": ["https://www.photopea.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
